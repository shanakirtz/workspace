		System.out.println("Press 1 if you like Rise Agsinst");
		System.out.println("Press 2 if you do not like Rise Against");
		num = scan.nextInt();
	
		while(num != 1)
		
			System.out.println("That is too bad");
			
		
			System.out.println("Good for you");
			
	}

}
