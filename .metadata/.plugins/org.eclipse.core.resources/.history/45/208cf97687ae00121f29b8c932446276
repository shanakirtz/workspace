import java.util.*;

public class RiseAgainst {


	public static void main(String[] args) {
	
		Scanner scan = new Scanner(System.in);
		int age;
		final int MAX_AGE = 30;
		
			System.out.println("Please enter your current age");
			age = scan.nextInt();
			
			while( age < MAX_AGE)
			{
				System.out.println("You are still young");
				System.out.println("Please enter another age");
				age = scan.nextInt();
			}
				System.out.println("You are getting old");
				System.out.println("Enter another age");
	}

}
