import java.util.*;
public class Arr {

	
	public static void main(String[] args) {
		
		String[] arr = new String[4];
		System.out.println("You will marry " + (getName(arr)));
	}
		
		public static String getName(String [] arr){
			
		//String[] arr = new String[4];
		Scanner scan = new Scanner(System.in);
		Random rand = new Random();
		int i;
		String name;
		
		
		for( i = 0; i < arr.length; i ++){
			System.out.println("Please enter name " + (i + 1) + " : " );
			arr[i] = scan.nextLine();	
			
		}
			name = arr[rand.nextInt(arr.length)];
			return name;
			
			
			//prints out names entered
			//for( i = 0;  i < arr.length; i++){ 
			//System.out.println("The names you entered are " + (arr[i]));
		//}
			
			

	}
	}


