import java.util.*;
public class Arr {

	
	public static void main(String[] args) {
		
		String[] arr = new String[4];
		Scanner scan = new Scanner(System.in);
		String name;
		//int i;
		
		for( int i = 0; i < arr.length; i ++){
			System.out.println("Please enter name " + (i + 1) + " : " );
			name = scan.nextLine();
		}
		
			for(int i = 0; i < arr.length; i++){
				System.out.println("You entered " + arr[i] );
			}
	}
	}


