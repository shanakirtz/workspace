import java.util.*;
public class Arr {

	
	public static void main(String[] args) {
		
		String[] arr = new String[4];
		Scanner scan = new Scanner(System.in);
		String name;
		
		for(int i = 0; i <= arr.length; i ++){
			System.out.println("Please enter name " + i );
			name = scan.nextLine();
		}
	}

}
