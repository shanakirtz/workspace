import java.util.*;
public class ForLoopTwo {


	public static void main(String[] args) {
	
		Scanner scan = new Scanner(System.in);
		int num = 1;
		int total = 0;
		
		while( num > 0){
		System.out.println("Please enter positive numbers or -99 to end");
		num = scan.nextInt();
		total = total + num;
		}
		System.out.println(total);
	}

}
