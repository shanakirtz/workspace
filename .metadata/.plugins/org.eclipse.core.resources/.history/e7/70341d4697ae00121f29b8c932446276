import java.util.*;
public class ForLoopTwo {


	public static void main(String[] args) {
	
		Scanner scan = new Scanner(System.in);
		int num = 0;
		int list = 0;
		
		while( num != -99){
		System.out.println("Please enter positive numbers or -99 to end");
		num = scan.nextInt();
		}
		System.out.println(list);
	}

}
