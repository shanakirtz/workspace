import java.util.*;
public class NestedLOOPS {

	
	public static void main(String[] args) {
	
		Scanner scan = new Scanner(System.in);
		int years;
		int month;
		int total =0;
		
		System.out.println("How many years are you collecting rain for?");
		years = scan.nextInt();
		for(int i = 0; i < 12; i++){
		System.out.println("How much rainfall for this month?");
		month = scan.nextInt();
		total = total + month;
		}
		System.out.println(total);
		
	}
	
		
}
