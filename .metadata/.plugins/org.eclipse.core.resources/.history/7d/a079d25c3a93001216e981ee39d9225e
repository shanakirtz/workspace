import java.util.*;

public class Options {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		Random rand = new Random();
		int counter;
		int number;
		
		for(counter = 1; counter <=5; counter++);
		number = rand.nextInt(5);
		System.out.println(number);

	}

}
